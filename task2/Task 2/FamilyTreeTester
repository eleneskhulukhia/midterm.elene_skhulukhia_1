package Task2;

import java.time.LocalDate;

public class FamilyTreeTester {
    public static void main(String[] args) {
        Person father = new Person();
        father.setName("John");
        father.setSurname("Doe");
        father.setDateOfBirth(LocalDate.of(1970, 5, 15));
        father.setRelation("Father");

        // Add more Person objects as needed

        FamilyTree familyTree = new FamilyTree();
        familyTree.addPerson(father);
        // Add more persons to the family tree

        familyTree.removePerson(father); // Remove a person from the family tree

        familyTree.printFamilyTree();
    }
}
package midterm.elene_skhulukhia_1.task1;

import java.util.ArrayList;
import java.util.List;

public class Fantasy extends AbstractFantasy implements Dream {

    private String studentName = "Elene skhulukhia";

    @Override
    public String getPiano1() {
        return "Piano";
    }

    @Override
    public String getTruck2() {
        return "Truck";
    }

    @Override
    public String methodThread3(String argCloud5) {
        return "Thread: " + argCloud5;
    }

    @Override
    public List<String> methodStyle4(String argTextiles6) {
        List<String> result = new ArrayList<>();
        result.add("Style 1: " + argTextiles6);
        result.add("Style 2: " + argTextiles6);
        return result;
    }

    @Override
    public String toString() {
        String result = "Student Name: " + studentName + "\n";
        result += "getPiano1(): " + getPiano1() + "\n";
        result += "getTruck2(): " + getTruck2() + "\n";
        result += "methodThread3(Cloud): " + methodThread3("Cloud") + "\n";
        result += "methodStyle4(Textiles): " + methodStyle4("Textiles");
        return result;
    }
}